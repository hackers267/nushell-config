# A REPL for Rust
export extern "main" [
   --disable-readline
   --ide-mode
   # Optimization level (0, 1 or 2) [default: ] 
   --opt
   # EDIT_MODE  [default: emacs] [possible values: emacs, vi]
   --edit-mode
   --help(-h)                   
   # Print help
   --version(-V)                
   # Print version
]
